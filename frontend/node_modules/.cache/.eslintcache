rd />\r\n    }\r\n  }\r\n\r\n  // Memoized Orb component for performance optimization\r\n  const orbMemo = useMemo(() => {\r\n    return <Orb />\r\n  }, [])\r\n\r\n  return (\r\n    <AuthProvider>\r\n      <GlobalProvider>\r\n        <Router>\r\n          <AppStyled bg={bg} className=\"App\">\r\n            {orbMemo} {/* Background animated orb */}\r\n            <Routes>\r\n              {/* Public routes */}\r\n              <Route path=\"/login\" element={<Login />} />\r\n              <Route path=\"/register\" element={<Register />} />\r\n              \r\n              {/* Protected routes */}\r\n              <Route \r\n                path=\"/dashboard\" \r\n                element={\r\n                  <ProtectedRoute>\r\n                    <MainLayout>\r\n                      <Navigation active={active} setActive={setActive} />\r\n                      <main>\r\n                        {displayData()} {/* Renders the selected component */}\r\n                      </main>\r\n                    </MainLayout>\r\n                  </ProtectedRoute>\r\n                } \r\n              />\r\n              \r\n              {/* Redirect root path to login */}\r\n              <Route path=\"/\" element={<Navigate to=\"/login\" replace />} />\r\n              \r\n              {/* Redirect any unknown routes to login */}\r\n              <Route path=\"*\" element={<Navigate to=\"/login\" replace />} />\r\n            </Routes>\r\n          </AppStyled>\r\n        </Router>\r\n      </GlobalProvider>\r\n    </AuthProvider>\r\n  );\r\n}\r\n\r\n// Styled component for App layout\r\nconst AppStyled = styled.div`\r\n  height: 100vh;\r\n  background-image: url(${props => props.bg});\r\n  position: relative;\r\n  \r\n  main {\r\n    flex: 1;\r\n    background: rgba(252, 246, 249, 0.78);\r\n    border: 3px solid #FFFFFF;\r\n    backdrop-filter: blur(4.5px);\r\n    border-radius: 32px;\r\n    overflow-x: hidden;\r\n\r\n    &::-webkit-scrollbar {\r\n      width: 0;\r\n    }\r\n  }\r\n`;\r\n\r\nexport default App;\r\n","C:\\Users\\soumy\\OneDrive\\Desktop\\ExpenseFlow_01\\frontend\\src\\context\\globalContext.js",["129"],[],"C:\\Users\\soumy\\OneDrive\\Desktop\\ExpenseFlow_01\\frontend\\src\\styles\\GlobalStyle.js",[],[],[],"C:\\Users\\soumy\\OneDrive\\Desktop\\ExpenseFlow_01\\frontend\\src\\styles\\Layouts.js",[],[],"C:\\Users\\soumy\\OneDrive\\Desktop\\ExpenseFlow_01\\frontend\\src\\Components\\Navigation\\Navigation.js",[],[],[],"C:\\Users\\soumy\\OneDrive\\Desktop\\ExpenseFlow_01\\frontend\\src\\Components\\Dashboard\\Dashboard.js",["130"],[],"import React, { useEffect } from 'react'\r\nimport styled from 'styled-components'\r\nimport { useGlobalContext } from '../../context/globalContext';\r\nimport History from '../../History/History';\r\nimport { InnerLayout } from '../../styles/Layouts';\r\nimport { dollar } from '../../utils/Icons';\r\nimport Chart from '../Chart/Chart';\r\n\r\nfunction Dashboard() {\r\n    const { totalExpenses, incomes, expenses, totalIncome, totalBalance, getIncomes, getExpenses } = useGlobalContext()\r\n\r\n    // Fetch income and expense data when the component mounts\r\n    useEffect(() => {\r\n        getIncomes()\r\n        getExpenses()\r\n    }, [])\r\n\r\n    // Handle empty arrays for min/max calculations\r\n    const getMinIncome = () => {\r\n        if (incomes.length === 0) return 0;\r\n        return Math.min(...incomes.map(item => item.amount));\r\n    };\r\n\r\n    const getMaxIncome = () => {\r\n        if (incomes.length === 0) return 0;\r\n        return Math.max(...incomes.map(item => item.amount));\r\n    };\r\n\r\n    const getMinExpense = () => {\r\n        if (expenses.length === 0) return 0;\r\n        return Math.min(...expenses.map(item => item.amount));\r\n    };\r\n\r\n    const getMaxExpense = () => {\r\n        if (expenses.length === 0) return 0;\r\n        return Math.max(...expenses.map(item => item.amount));\r\n    };\r\n\r\n    return (\r\n        <DashboardStyled>\r\n            <InnerLayout>\r\n                <h1>All Transactions</h1>\r\n                <div className=\"stats-con\">\r\n                    <div className=\"chart-con\">\r\n                        <Chart /> {/* Display the chart component */}\r\n                        <div className=\"amount-con\">\r\n                            {/* Display total income, expenses, and balance */}\r\n                            <div className=\"income\">\r\n                                <h2>Total Income</h2>\r\n                                <p>{dollar} {totalIncome()}</p>\r\n                            </div>\r\n                            <div className=\"expense\">\r\n                                <h2>Total Expense</h2>\r\n                                <p>{dollar} {totalExpenses()}</p>\r\n                            </div>\r\n                            <div className=\"balance\">\r\n                                <h2>Total Balance</h2>\r\n                                <p>{dollar} {totalBalance()}</p>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n\r\n                    {/* Display transaction history and min/max salary & expense */}\r\n                    <div className=\"history-con\">\r\n                        <History />\r\n                        <h2 className=\"salary-title\">Min <span>Salary</span> Max</h2>\r\n                        <div className=\"salary-item\">\r\n                            <p>₹{getMinIncome()}</p>\r\n                            <p>₹{getMaxIncome()}</p>\r\n                        </div>\r\n                        <h2 className=\"salary-title\">Min <span>Expense</span> Max</h2>\r\n                        <div className=\"salary-item\">\r\n                            <p>₹{getMinExpense()}</p>\r\n                            <p>₹{getMaxExpense()}</p>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </InnerLayout>\r\n        </DashboardStyled>\r\n    )\r\n}\r\n\r\n// Styled components for dashboard layout\r\nconst DashboardStyled = styled.div`\r\n    .stats-con {\r\n        display: grid;\r\n        grid-template-columns: repeat(5, 1fr);\r\n        gap: 2rem;\r\n\r\n        .chart-con {\r\n            grid-column: 1 / 4;\r\n            height: 400px;\r\n            \r\n            .amount-con {\r\n                display: grid;\r\n                grid-template-columns: repeat(4, 1fr);\r\n                gap: 2rem;\r\n                margin-top: 2rem;\r\n\r\n                .income, .expense {\r\n                    grid-column: span 2;\r\n                }\r\n\r\n                .income, .expense, .balance {\r\n                    background: #FCF6F9;\r\n                    border: 2px solid #FFFFFF;\r\n                    box-shadow: 0px 1px 15px rgba(0, 0, 0, 0.06);\r\n                    border-radius: 20px;\r\n                    padding: 1rem;\r\n                    \r\n                    p {\r\n                        font-size: 3.5rem;\r\n                        font-weight: 700;\r\n                    }\r\n                }\r\n\r\n                .balance {\r\n                    grid-column: 2 / 4;\r\n                    display: flex;\r\n                    flex-direction: column;\r\n                    justify-content: center;\r\n                    align-items: center;\r\n                    \r\n                    p {\r\n                        color: var(--color-green);\r\n                        opacity: 0.6;\r\n                        font-size: 4.5rem;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n\r\n        .history-con {\r\n            grid-column: 4 / -1;\r\n\r\n            h2 {\r\n                margin: 1rem 0;\r\n                display: flex;\r\n                align-items: center;\r\n                justify-content: space-between;\r\n            }\r\n\r\n            .salary-title {\r\n                font-size: 1.2rem;\r\n\r\n   